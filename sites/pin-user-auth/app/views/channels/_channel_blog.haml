-if logged_in? && channel.is_include_users_or_creator?(current_user)
  .blog-post-buttons
    =minibutton '发新日志',pin_url_for('pin-user-auth',"channels/#{channel.id}/new_blog_post")

.newsfeed
  -mplist feeds,:for=>Feed do |feed|
    =render :partial=>'index/homepage/feeds/feed_info_in_blog',:locals=>{:feed=>feed}