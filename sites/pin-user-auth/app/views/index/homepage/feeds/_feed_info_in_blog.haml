-ea = EmailActor.new(feed.email)

.f.darkbg{:'data-id'=>"#{feed.id}"}
  .blog-head
    .title
      =link_to feed.content,'javascript:;'
    .meta
      .avatar
        =avatar ea.email,:mini
      .name
        =link_to ea.name,ea.actor
      .posted-at
        =jtime feed.created_at

-html = feed.html
-if !html.blank?
  .blog-html.darkbg
    =html
=render :partial=>'index/homepage/feeds/info_parts/info_time_and_ops',:locals=>{:feed=>feed}